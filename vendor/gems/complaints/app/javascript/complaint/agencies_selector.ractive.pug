    | {{#all_agencies_in_sixes:i}}
    .row.agency
      | {{#.}}
      .col-md-1
        label(for = 'agency_{{.id}}')
          span('data-toggle'="tooltip", 'data-placement'="bottom", title='{{.full_name}}')
            | {{.name}}
