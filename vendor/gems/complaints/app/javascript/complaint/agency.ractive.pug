.agency {{description}}

script.
  export default Ractive.extend({
    template : $TEMPLATE,
    computed : {
      description() {
        const agency = _(this.get('all_agencies')).findWhere({id : this.get('agency_id')});
        if (_.isUndefined(agency)) { return null; } else { return agency.description; }
      }
    },
    data(){ return {} },
  });

