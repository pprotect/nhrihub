span.agency
  | {{name}}

script.
  export default Ractive.extend({
    template : $TEMPLATE,
    computed : {
      name() {
        const agency = _(this.get('all_agencies')).findWhere({id : this.get('id')});
        if (_.isUndefined(agency)) { return null; } else { return agency.name; }
      }
    },
    data(){ return {} },
  });

