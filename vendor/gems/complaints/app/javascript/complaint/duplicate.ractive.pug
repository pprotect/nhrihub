span.duplicate_case_reference
  a(href='{{link}}') {{case_reference}}

script(type='text/javascript').
  export default Ractive.extend({
    template : $TEMPLATE,
    computed : {
      link(){
        if(this.get('id')){
          return Routes.complaint_path('en', this.get('id'))
        }else{
          return null
        }
      },
    }
  })
